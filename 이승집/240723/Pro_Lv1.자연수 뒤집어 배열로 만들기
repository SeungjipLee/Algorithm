public class Main {
    public static void main(String[] args) {
        long n = 12345;
        String num = String.valueOf(n);
        int[] answer = new int[num.length()];

        for (int i = num.length()-1; i > -1; i--) {
            answer[num.length() - 1 - i] = num.charAt(i) - '0';
        }
    }
}